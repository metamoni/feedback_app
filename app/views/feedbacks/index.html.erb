<div>
  <h1 class="font-bold text-4xl mb-4">All feedback</h1>

  <% if @feedbacks.empty? %>
    <p>No feedback recorded yet</p>
  <% else%>
    <% @feedbacks.each do |f| %>
      <div class="mb-4">
        <p>Feedback: <%= f.content %>
        <p>Status: <%= f.status.humanize %>
      </div>
    <% end %>
  <% end %>
</div>
